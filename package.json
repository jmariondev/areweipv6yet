{
  "name": "areweipv6yet",
  "version": "1.0.0",
  "description": "Track IPv6 adoption across popular services",
  "type": "module",
  "scripts": {
    "build": "eleventy",
    "validate": "node scripts/validate-data.js",
    "check-ipv6": "node scripts/check-ipv6.js",
