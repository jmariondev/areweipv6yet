    "eleventy:serve": "eleventy --serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "csso": "^5.0.5",
    "html-minifier-terser": "^7.2.0",
    "js-yaml": "^4.1.1",
    "terser": "^5.44.0",
    "wrangler": "^3.0.0"
  }
}
